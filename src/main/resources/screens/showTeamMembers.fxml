<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mikolajm.project.sportclubui.screenController.ShowTeamMembersController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="800.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="400.0" spacing="15.0">
               <children>
                  <Label fx:id="teamName" text="Team name">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Label>
                  <ImageView fx:id="teamLogo" fitHeight="91.0" fitWidth="103.0" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="membersNum" prefHeight="35.0" prefWidth="117.0" text="Members: NUM" />
               </children>
            </HBox>
            <ScrollPane prefHeight="700.0" prefWidth="500.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="670.0" prefWidth="500.0">
                     <children>
                        <VBox spacing="10.0" fx:id="membersColumn" alignment="TOP_CENTER" prefHeight="700.0" prefWidth="500.0" />
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
         </children></VBox>
   </children>
</AnchorPane>
