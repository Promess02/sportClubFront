<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mikolajm.project.sportclubui.screenController.ErrorMessageController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="300.0">
         <children>
            <ImageView fx:id = "exclamationImg" fitHeight="118.0" fitWidth="119.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id = "errorMsg" prefHeight="131.0" prefWidth="170.0" text="error message" wrapText="true">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
