<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="fullView" prefHeight="100.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mikolajm.project.sportclubui.screenController.generalScreens.SocialsNodeController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="250.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="150.0" spacing="3">
               <children>
                  <Label fx:id="nameLabel" prefHeight="25.0" prefWidth="145.0" text="nameLabel" wrapText="true">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                  <Label fx:id="urlLabel" prefHeight="49.0" prefWidth="145.0" text="urlLabel" wrapText="true">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                   <Label fx:id="locationLabel" prefHeight="25.0" prefWidth="145.0" text="locationLabel" wrapText="true">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
               </children>
               <HBox.margin>
                  <Insets left="3.0" />
               </HBox.margin>
            </VBox>
            <ImageView fx:id="imageView" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
   </children>
</AnchorPane>
