<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mikolajm.project.sportclubui.screenController.FireTrainerController">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="300.0">
            <children>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label prefHeight="95.0" prefWidth="93.0" text="Trainer: ">
                            <HBox.margin>
                                <Insets left="10.0" right="10.0" top="5.0" />
                            </HBox.margin>
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                        <ChoiceBox fx:id="trainerCb" prefHeight="50.0" prefWidth="150.0">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                </FXCollections>
                            </items>
                        </ChoiceBox>
                    </children>
                </HBox>
                <Button fx:id="fireBtn" mnemonicParsing="false" prefHeight="76.0" prefWidth="139.0" text="Fire trainer">
                    <font>
                        <Font size="19.0" />
                    </font></Button>
            </children></VBox>
    </children>

</AnchorPane>
