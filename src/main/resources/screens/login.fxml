<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="150.0" prefWidth="651.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mikolajm.project.sportclubui.screenController.LoginController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <StackPane prefHeight="137.0" prefWidth="631.0">
        <VBox spacing="10.0">
            <GridPane prefHeight="101.0" prefWidth="631.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="61.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <Label text="Email:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <TextField fx:id="user" prefHeight="39.0" prefWidth="228.0" promptText="enter email" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <PasswordField fx:id="password" prefHeight="34.0" prefWidth="228.0" promptText="enter password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            </GridPane>
            <StackPane prefHeight="57.0" prefWidth="631.0">
                <Button fx:id="loginButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" prefHeight="41.0" prefWidth="73.0" text="Login" StackPane.alignment="CENTER_RIGHT" />
            </StackPane>
        </VBox>
    </StackPane>
</StackPane>
